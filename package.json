{
    "name": "mp",
    "displayName": "MP",
    "description": "MP syntax highlighting",
    "version": "0.0.1",
    "publisher": "ahernandez",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "url": "https://github.com/saffronfries/mp-syntax"
    },
    "contributes": {
        "grammars": [
            {
                "language": "html",
                "scopeName": "source.mp",
                "path": "./syntaxes/mp.tmLanguage.json",
                "injectTo": ["text.html"],
               "embeddedLanguages": {
                    "meta.embedded.block.perl": "source.perl"
                } 
            }
        ]
    }
}